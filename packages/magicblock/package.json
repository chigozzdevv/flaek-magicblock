{
  "name": "@flaek/magicblock",
  "version": "0.1.1",
  "description": "Flaek SDK for Magicblock job execution",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@magicblock-labs/ephemeral-rollups-sdk": "^0.8.0",
    "@solana/web3.js": "^1.98.4",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "flaek",
    "magicblock",
    "solana",
    "sdk",
    "ephemeral-rollups",
    "tee",
    "per",
    "er"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chigozzdevv/flaek-magicblock"
  },
  "homepage": "https://flaek.dev",
  "bugs": {
    "url": "https://github.com/chigozzdevv/flaek-magicblock/issues"
  }
}
